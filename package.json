{
	"name": "ticketing",
	"displayName": "ticketing",
	"description": "Ticketing system prototype.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ticketing.helloWorld", 
		"onCommand:ticketing.start"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ticketing.helloWorld",
				"title": "Hello World"
			}, 
			{
				"command": "ticketing.start",
				"title": "Start Ticket"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
